{
  "name": "showtime",
  "version": "0.0.0",
  "homepage": "https://mamadfar.github.io/showtime-netflix-like__angular/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run showtime:serve-ssr",
